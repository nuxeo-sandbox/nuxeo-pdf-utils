<package type="addon" name="nuxeo-pdf-utils" version="@VERSION@">
  <title>Nuxeo PDF Utils Operations</title>
  <description>
    <p>This addon activates the Nuxeo PDF Utils Operations addon.</p>
    <p>This addon adds new features as explained <a href="https://github.com/nuxeo-sandbox/nuxeo-pdf-utils">here</a></p>
  </description>
  <home-page>https://github.com/nuxeo-sandbox/nuxeo-pdf-utils</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <hotreload-support>false</hotreload-support>
  <require-terms-and-conditions-acceptance>false</require-terms-and-conditions-acceptance>
  <nuxeo-validation>waiting_nuxeo_certified</nuxeo-validation>
  <production-state>development</production-state>
  <supported>false</supported>
  <platforms>
    <platform>cap-@DISTRIB_VERSION@</platform>
    <platform>cap-@DISTRIB_VERSION@-HF</platform>
    <platform>cap-7.10</platform>
    <platform>cap-7.10-HF*</platform>
    <platform>cap-8.2</platform>
    <platform>cap-8.3</platform>
  </platforms>
<!-- 
  <dependencies>
    <package>nuxeo-adullact-service:@VERSION@</package>
  </dependencies>
 -->
  <license>Apache License, Version 2.0</license>
  <license-url>http://www.apache.org/licenses/LICENSE-2.0</license-url>
</package>
